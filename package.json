{
  "name": "test_frontend",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"npm run start-webpack-dev-server\" \"npm run start-dev-server\"",
    "start-webpack-dev-server": "node ./webpack-dev-server.js",
    "start-dev-server": "nodemon ./src/server --exec babel-node",
    "build": "rimraf dist/*.js && npm run build-webpack && npm run build-server",
    "build-webpack": "NODE_ENV=production webpack --progress --colors",
    "build-server": "NODE_ENV=production babel src -d dist",
    "start": "NODE_ENV=production node dist/server",
    "lint": "eslint -c .eslintrc src",
    "lint-fix": "eslint -c .eslintrc src --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "http-proxy": "^1.16.2",
    "lodash.concat": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^1.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.1.1"
  },
  "pre-commit": [
    "lint"
  ]
}
